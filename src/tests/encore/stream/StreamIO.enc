module StreamIO
active class StreamIO
  stream produceInt(n : int) : int
    var i = n
    while i > 0 do
      yield(i)
      i = i - 1
    end
  end
  stream produceReal(n : int) : real
    var i = n
    while i > 0 do
      yield(i * 1.0)
      i = i - 1
    end
  end
  def printStreamReal(sr : Stream[real]) : unit
    var str = sr
    while not(eos(str)) do
      println(get(str))
      str = getNext(str)
    end
  end
  def printStreamInt(si : Stream[int]) : unit
