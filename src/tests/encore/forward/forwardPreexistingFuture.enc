-- TEST FORWARD, PREEXISTING FUTURE CHAINING
class Foo
  def two(x : int) : int
    42*x
  end
end

class Base
  def one(arg : Fut[int]) : int
    forward(arg ~~> fun(x:int) => x * 10)
  end
end

class Main
  def main() : unit
    val arg = (new Foo)!two(1)
    print("{}\n",get((new Base)!one(arg)))
  end
end
