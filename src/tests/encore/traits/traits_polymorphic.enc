def id<a>(x : a) : a
  x

class Foo<t>
  def init(x : t) : void
    ()

trait T<t> {
  require x : t
  def foo() : void {
    val arr = new [t](10);
    val x = arr[0] : t;
    new Foo<t>(x);
    val f = id<t>;
    f(x);
    id(x);
    EMBED (t)
      #{x};
    END;
  }
}

passive class C : T<int> {
  x : int
}

class Main {
  def main() : void {
    -- ()
    let
      c = new C
    in {
      c.x = 1;
      print c.x;
    }
  }
}
