fun id[a](x : a) : a
  x
end
local trait T[t]
  require var x : t
  def foo() : unit
    val arr = new [t](10)
    val x = arr(0) : t
    new Foo[t](x)
    val f = id[t]
    f(x)
    id(x)
    EMBED (t)
      #{x};
    END
  end
end
active class Foo[t]
  def init(x : t) : unit
    ()
  end
end
local class C : T[int]
  var x : int
end
active class Main
  def main() : unit
    let
      c = new C
    in
      c.x = 1
      println(c.x)
    end
  end
end
