trait T
  def getClosure() : () -> void
    \ () -> print "You have finally reached closure!"

passive class Foo : T
passive class Bar : T

class Main
  def main() : void {
    val x = new Foo();
    val y = new Bar();
    val f1 = x.getClosure();
    val f2 = y.getClosure();
    f1();
    f2();
  }