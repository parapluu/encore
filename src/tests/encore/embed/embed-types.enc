passive class Container<t>
  f : t
  def write(f : t) : void
    this.f = f

  def read() : t
    this.f

class Main
  def main() : void
    let c = new Container<EMBED char* END>
    in{
      c.write(EMBED (EMBED char* END) "Hello"; END);
      let s = c.read() in
        EMBED (void) puts(#{s}); END
    }