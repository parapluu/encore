class Worker
  def work() : int
    42

class Lazy
  def work() : int {
    if true then
      forward ((new Worker).work())
    else
      100;
    print "This should never run!";
    0
  }
  def foo() : int
    this.work()

class Main
  def main() : void {
    print(get (new Lazy).work());
    (new Lazy)!work();
    (new Lazy)!foo();
    print(get (new Lazy).foo());
  }
