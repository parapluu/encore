passive class Foo
  def bar() : unit
    repeat i <- 50 do
      print ("{} ",i)
      if i>5 then
        break
        print ("Never show")
      end
    end
  end
end

class Main
  def main() : unit
    repeat i <- 50 do
      print ("{} ",i)
      if i>5 then
        break
        print ("Never show")
      end
    end
    print ("Done!\n")
    (new Foo).bar()
    print ("Done!\n")
  end
end
