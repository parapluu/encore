class Foo {
  def work() : void {
    println("working")
  }
}

class Client {
  def start() : void {
    let
      -- peer is not supported by the compiler yet
      -- f1 = peer Foo
      -- f2 = peer Foo
      f1 = new Foo
      f2 = new Foo
    in {
      f1.work();
      f2.work();
    }
  }
}

class Main {
  def main() : void
    (new Client).start()
}
