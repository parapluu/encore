#!/usr/bin/env encorec -run

-- a Sequence of Integers
class IntSeq

  stream start(var fr : int, to : int) : int {
    while fr <= to {
      yield fr;
      fr = fr+1
    };
  }

class Main
  def main() : void {
    var lst = 0;
    var str = (new IntSeq).start(1,1000000);
    while not eos str {
      lst = get str;
      str = getNext str;
    };
    print lst
  }
