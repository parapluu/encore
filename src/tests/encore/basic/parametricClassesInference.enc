def id<a>(x: a): a x

class Cell<a>
  value : a
  def init(value : a) : void
    this.value = value

  def getVal() : a
    this.value

  def getVal2() : void
    new Cell(this.value)

  def clone(value : a): void
    new Cell(value)

class Pair<a, b>
  fst : a
  snd : b
  def init(fst : a, snd : b) : void{
    this.fst = fst;
    this.snd = snd;
  }
  def getFst() : a
    this.fst

  def getSnd() : b
    this.snd

  def liftJust<d>(x: d): Maybe d { Just x; }

class Main
  def main() : void{
    let p = new Pair(42, "fortytwo")
        c = new Cell(p)
        pp = get c.getVal()
    in {
      print get pp.getFst();
      print get pp.getSnd();
    }
  }
