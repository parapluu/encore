class Main
  def main() : void {
    for i in [1..9] {
      println("The square of {} is {}", i, i*i);
    };
    let row = [false, true];
    for b1 in row {
      for b2 in row {
        println("{} | {}", (b1, b2), b1 and b2);
      }
    };
    println("void = {}", ());
    println("Zero is {}", Nothing : Maybe int);
    println("The answer is {}", Just 42);
  }