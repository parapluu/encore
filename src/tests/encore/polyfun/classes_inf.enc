fun pCreateTuple[v, k](key : k, value : v) : (k, v)
  (key, value)
end
fun fAppend[t](x : t, y : t) : [t]
  [x, y]
end
fun createTest[t](arg : t) : Test[t]
  new Test[t](arg)
end
class Test[a]
  var arg : a
  def init(arg : a) : void
    this.arg = arg
  end
  def appendToArray(arg : a) : [a]
    fAppend(this.arg, arg)
  end
  def toTuple(arg : a) : (a, a)
    pCreateTuple(this.arg, arg)
  end
  def getArg() : a
    this.arg
  end
end
class Main
  def main() : void
    val t = createTest("Create Test of type 'String'")
    println(get(t ! getArg()))
    val arr = get(t ! appendToArray("Adding a new string"))
    repeat i <- |arr| do
      println(arr(i))
    end
    val tup = get(t ! toTuple("Adding to tuple"))
    println(tup)
  end
end

