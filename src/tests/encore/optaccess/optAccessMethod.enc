passive class ContainerX
  val x: X

  def init(x: X): unit
    this.x = x
  end

  def getConst(): int
    42
  end
end

class X
  def getInt(): int
    42
  end
end

class Main
  def main() : unit
    val x = Just(new X)
    val mf = x?!getInt()
    match mf with
      case Just(f) => println(get(f))
      case Nothing => println("Error")
    end

    val cont = Just(new ContainerX(new X))
    val result = cont?.x?!getInt()
    match result with
      case Just(f) => println(get(f))
      case Nothing => println("Error")
    end
  end
end
